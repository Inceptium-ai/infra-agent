# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# Project Configuration
PROJECT_NAME=infra-agent
ENVIRONMENT=dev

# Domain Configuration
DOMAIN_NAME=example.com
HOSTED_ZONE_ID=Z1234567890ABC

# EKS Configuration
EKS_CLUSTER_NAME=infra-agent-dev-cluster
EKS_CLUSTER_VERSION=1.34

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_REGION=us-east-1

# GitHub Configuration (for CI/CD)
GITHUB_ORG=Inceptium-ai
GITHUB_REPO=infra-agent

# Observability URLs (populated after deployment)
GRAFANA_URL=https://grafana.infra-agent-dev.example.com
LOKI_URL=https://loki.infra-agent-dev.example.com
TEMPO_URL=https://tempo.infra-agent-dev.example.com

# Admin Console
HEADLAMP_URL=https://console.infra-agent-dev.example.com

# Database (populated after RDS deployment)
DATABASE_HOST=infra-agent-dev-postgres.xxxxx.us-east-1.rds.amazonaws.com
DATABASE_PORT=5432
DATABASE_NAME=infra_agent
DATABASE_USER=admin
DATABASE_PASSWORD=your_db_password_here

# S3 Buckets (populated after deployment)
VELERO_BUCKET=infra-agent-dev-velero-backups
LOGS_BUCKET=infra-agent-dev-logs

# Allowed CIDR for Bastion Access (corporate VPN)
ALLOWED_CIDR_BLOCKS=10.0.0.0/8,192.168.0.0/16
